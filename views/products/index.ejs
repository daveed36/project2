<!DOCTYPE html>
<html>
  <head>
    <%- include ('../partials/head') %>
  </head>
  <body>
    <h1>Hello MF</h1>
    <nav>
    </nav>
    <div class="container">
      <div class="products">
        <% products.forEach( product => { %>
          <div class="card">
            <h5> <%= product.name %></h5>
            <a href="/products/<%=product.id%>">
              <img src="<%= product.img %>" alt="An image of <%= product.name %>" /></a>
              <h6>Price: $<%=product.price %></h6>
          </div>
        <% }) %>
      </div>
    </div>
  </body>
</html>
